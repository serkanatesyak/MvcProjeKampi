@using EntityLayer
@model List<About>
@using DataAccessLayer.Concrete

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h2>Hakkımızda Veri Listesi</h2>
<table class="table table-bordered">
    <tr>
        <td>ID</td>
        <td>Hakkımda Başlığı</td>
        <td>Aktif Yap</td>
        <td>Pasif Yap</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutID</td>
            <td>Başlık</td>
            <td><a href="#" class="btn btn-warning">Aktif Yap</a></td>
            <td><a href="#" class="btn btn-danger">Pasif Yap</a></td>
        </tr>
    }
</table>


@Html.Action("AboutPartial", "About")